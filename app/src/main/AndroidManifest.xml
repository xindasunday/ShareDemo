<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="custom.sunday.com.sharedemo">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".HomeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".base.BaseActivity" />
        <activity android:name=".activity.SettingItemActivity" />
        <activity android:name=".activity.PathActivity" />
        <activity android:name=".activity.RefreshActivity"/>
        <activity android:name=".activity.ObjectBoxActivity"/>
    </application>

</manifest>